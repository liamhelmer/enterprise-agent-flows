{
  "name": "claude-plugins",
  "version": "1.0.0",
  "description": "A marketplace for Claude Code plugins - discover, publish, and manage Claude plugins",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "claude-plugins": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "validate": "node dist/cli/index.js validate",
    "prepare": "npm run build"
  },
  "keywords": [
    "claude",
    "claude-code",
    "plugins",
    "marketplace",
    "skills",
    "anthropic",
    "ai",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/liamhelmer/claude-plugins"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0",
    "semver": "^7.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.0",
    "@types/semver": "^7.5.6",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
